{
  "name": "kopnik-node",
  "version": "0.0.1",
  "description": "Official kopnik.org server",
  "scripts": {
    "start": "node --require ts-node/register -r tsconfig-paths/register src/index.ts",
    "test": "jest  --runInBand --testTimeout=10000",
    "typeorm": "node --require ts-node/register -r tsconfig-paths/register -r dotenv/config ./node_modules/typeorm/cli.js",
    "typeorm:schema:log": "node --require ts-node/register -r tsconfig-paths/register -r dotenv/config ./node_modules/typeorm/cli.js schema:log",
    "typeorm:schema:drop": "node --require ts-node/register -r tsconfig-paths/register -r dotenv/config ./node_modules/typeorm/cli.js schema:drop",
    "typeorm:migration:create": "node --require ts-node/register -r tsconfig-paths/register -r dotenv/config ./node_modules/typeorm/cli.js migration:create",
    "typeorm:migration:generate": "node --require ts-node/register -r tsconfig-paths/register -r dotenv/config ./node_modules/typeorm/cli.js migration:generate",
    "typeorm:migration:run": "node --require ts-node/register -r tsconfig-paths/register -r dotenv/config ./node_modules/typeorm/cli.js migration:run",
    "env:test": "cp .env.test .env",
    "env:development": "cp .env.development .env",
    "start:test": "npm run env:test && npm start",
    "typeorm:migration:run:test": "npm run env:test && npm run typeorm:migration:run",
    "db": "docker-compose up db",
    "db:development": "docker-compose --env-file=.env.development -p=development up db",
    "db:test": "docker-compose --env-file=.env.test -p=test up db"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.15",
    "@types/node": "^8.0.29",
    "@types/supertest": "^2.0.10",
    "jest": "^26.4.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0"
  },
  "dependencies": {
    "@types/bunyan": "^1.8.6",
    "@vk-io/authorization": "^1.0.0",
    "body-parser": "^1.18.1",
    "bunyan": "^1.8.14",
    "bunyan-logstash-tcp": "^1.0.0",
    "cors": "^2.8.5",
    "cross-fetch": "^3.0.6",
    "date-fns": "^2.15.0",
    "dotenv": "^8.2.0",
    "easyvk": "^2.8.2",
    "express": "^4.15.4",
    "express-async-errors": "^3.1.1",
    "express-cls-hooked": "^0.3.8",
    "glob": "^6.0.4",
    "https": "^1.0.0",
    "i18next": "^19.8.4",
    "inversify": "^5.0.1",
    "js-base64": "^3.6.0",
    "lodash": "^4.17.20",
    "md5": "^2.3.0",
    "nodemon": "^2.0.4",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.10",
    "sql-formatter": "^2.3.3",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "^0.2.29",
    "typeorm-naming-strategies": "^2.0.0",
    "typeorm-sequence": "^1.0.4",
    "vk-io": "^4.1.1"
  }
}
